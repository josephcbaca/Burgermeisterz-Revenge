<div class="container">
  <div class="hiddenDiv"></div>
  <div class="row">
    <img id="burgerIcon" src="/assets/burgermeister-icon/burger.png" alt="Red Burger Icon"></img>
    <h1 class="col-12 text-center" id="title">Burgermeisterz Revenge</h1>
  </div>

  <div class="row mt-5">

    <div class="card addCard col-4">
      <h2 class="addText">Add a Boiga</h2>
      <form class="create-form">
        <div class="form-group">
          <label class="addText" for="ca">Boiga Name:</label>
          <input class="mt-3 addInput" type="text" id="ca" name="name" placeholder="Type burger here">
        </div>
        <div class="form-group">
          <button class="btn btn-outline addButton" type="submit">Add Boiga</button>
        </div>
      </form>
    </div>

    <div class="col-2 blankDiv"></div>

    <div class="card burgerLists col-6">
      <h2>Boigas Wishlist</h2>
      <div id="burger-wishlist"></div>
      <h2>Boigas Demolished</h2>
      <div id="burger-demolish-list"></div>
    </div>
  </div>
</div>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/@mojs/core"></script>
<script src="/js/burgers.js"></script>